{
  "name": "auctionfity_marketplace",
  "author": "Patricio J. Gerpe",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "chai": "^4.3.6",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "fs": "^0.0.1-security",
    "prettier": "^2.7.1",
    "solhint": "^3.3.7"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.2",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.3",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "hardhat": "^2.10.1",
    "moment": "^2.29.4",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "tsc",
    "init": "node ./scripts/address.ts",
    "start": "nodemon ./src/index.ts",
    "start:node": "npx hardhat node",
    "test": "npx hardhat test",
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat deploy",
    "deploy:dev": "npx hardhat run --network localhost scripts/deploy.ts",
    "deploy:stag": "npx hardhat run --network goerli scripts/deploy.ts",
    "lint:ts": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "format": "prettier --write '*/**/*.{js,sol,json,md,ts}'",
    "format:check": "prettier --check '*/**/*.{js,sol,json,md,ts}'"
  }
}
